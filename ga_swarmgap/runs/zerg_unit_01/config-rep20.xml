<ga_swarmgap>

    <io>
        <output-dir value="zerg_unit_score-rep20" />
    </io>

	<parameters>
		<enemy value="zerg"/>
		<popsize value='30' />
		<elitism value='true' />
		<tournament-size value='2' />
		<p-crossover value='.9' />
		<p-mutation value='.01' />
		<generations value='50' />
		
		<reliab-threshold value='0.5' />
		<p-eval-above-thresh value='1.0' />
	</parameters>

	<fitness>
		<function value='unit_score' />
	</fitness>     
	           
	<externals>
		<random-seed value='20' />
		<repetitions value='1' />
	</externals>            
</ga_swarmgap>
