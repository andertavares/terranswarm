<ga_swarmgap>

    <io>
        <output-dir value="test" />
    </io>

	<parameters>
        <enemy value='Zerg' />
		<popsize value='1000' />
		<elitism value='true' />
		<tournament-size value='5' />
		<p-crossover value='.9' />
		<p-mutation value='.01' />
		<generations value='1000' />
		
		<reliab-threshold value='0.6' />
        <p-eval-above-thresh value='0.1' />
	</parameters>

    <fitness>
        <function value="score_ratio" />
    </fitness>
	           
	<externals>
		<random-seed value='79' />
		<repetitions value='1' />
	</externals>            
</ga_swarmgap>