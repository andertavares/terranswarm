<ga_swarmgap>
<!-- simple execution to test the fitness function based on victory ratio
only 2 generations with 2 individuals is executed -->

    <io>
        <output-dir value="test_victory_ratio" />
    </io>

	<parameters>
        <enemy value='terran' />
		<popsize value='2' />
		<elitism value='true' />
		<tournament-size value='2' />
		<p-crossover value='.9' />
		<p-mutation value='.01' />
		<generations value='2' />
		
		<reliab-threshold value='0.6' />
        <p-eval-above-thresh value='1.0' />
	</parameters>

    <fitness>
        <function value="victory_ratio" />
        <num_matches value="3" />
    </fitness>
	           
	<externals>
		<random-seed value='6' />
		<repetitions value='1' />
	</externals>            
</ga_swarmgap>