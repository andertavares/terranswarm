<ga_swarmgap>
<!-- increasing tournament size and reducing crossover compared to protoss17-->
    <io>
        <output-dir value="protoss20" />
    </io>

	<parameters>
		<enemy value='protoss'/>
		<popsize value='25' />
		<elitism value='true' />
		<tournament-size value='5' />
		<p-crossover value='.8' />
		<p-mutation value='.01' />
		<generations value='100' />
		
		<reliab-threshold value='0.8' />
        <p-eval-above-thresh value='1.0' />
	</parameters>     
	           
	<externals>
		<random-seed value='20' />
		<repetitions value='1' />
	</externals>            
</ga_swarmgap>