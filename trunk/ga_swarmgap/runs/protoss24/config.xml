<ga_swarmgap>
<!-- protoss17 with less crossover and mutation-->
    <io>
        <output-dir value="protoss24" />
    </io>

	<parameters>
        <enemy value="protoss"/>
		<popsize value='30' />
		<elitism value='true' />
		<tournament-size value='2' />
		<p-crossover value='.6' />
		<p-mutation value='.005' />
		<generations value='100' />
		
		<reliab-threshold value='0.8' />
        <p-eval-above-thresh value='1.0' />
	</parameters>     
	           
	<externals>
		<random-seed value='24' />
		<repetitions value='1' />
	</externals>            
</ga_swarmgap>