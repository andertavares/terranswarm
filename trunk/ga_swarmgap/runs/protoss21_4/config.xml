<ga_swarmgap>
<!-- Adding fitness estimation to protoss17 in 100 generations-->
    <io>
        <output-dir value="protoss21_4" />
    </io>

	<parameters>
		<enemy value='protoss'/>
		<popsize value='30' />
		<elitism value='true' />
		<tournament-size value='2' />
		<p-crossover value='.9' />
		<p-mutation value='.01' />
		<generations value='100' />
		
		<reliab-threshold value='0.8' />
        <p-eval-above-thresh value='0.1' />
	</parameters>     
	           
	<externals>
		<random-seed value='214' />
		<repetitions value='1' />
	</externals>            
</ga_swarmgap>