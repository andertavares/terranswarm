<ga_swarmgap>
<!-- evaluating all from protoss6 with 50 generations-->
    <io>
        <output-dir value="protoss17" />
    </io>

	<parameters>
		<popsize value='30' />
		<elitism value='true' />
		<tournament-size value='2' />
		<p-crossover value='.9' />
		<p-mutation value='.01' />
		<generations value='100' />
		
		<reliab-threshold value='0.8' />
        <p-eval-above-thresh value='1.0' />
	</parameters>     
	           
	<externals>
		<random-seed value='17' />
		<repetitions value='1' />
	</externals>            
</ga_swarmgap>