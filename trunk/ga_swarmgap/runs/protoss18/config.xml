<ga_swarmgap>
<!-- evaluating all individuals from toss8 in 50 generations-->
    <io>
        <output-dir value="protoss18" />
    </io>

	<parameters>
		<popsize value='30' />
		<elitism value='true' />
		<tournament-size value='2' />
		<p-crossover value='.9' />
		<p-mutation value='.005' />
		<generations value='50' />
		
		<reliab-threshold value='0.8' />
        <p-eval-above-thresh value='1.0' />
	</parameters>     
	           
	<externals>
		<random-seed value='18' />
		<repetitions value='1' />
	</externals>            
</ga_swarmgap>